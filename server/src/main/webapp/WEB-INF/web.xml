<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>ndss - Network Data Storage Server</display-name>
  <servlet>
    <servlet-name>ndss</servlet-name>
    <servlet-class>eu.balewski.NDSServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ndss</servlet-name>
    <url-pattern>/ndss</url-pattern>
  </servlet-mapping>
</web-app>
